<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.app.qxjgl.business.dao.QxjModleDeptDao">

	<select id="queryObject" resultType="com.css.app.qxjgl.business.entity.QxjModleDept">
		select * from QXJ_MODLE_DEPT where ID = #{value}
	</select>

	<select id="queryList" resultType="com.css.app.qxjgl.business.entity.QxjModleDept">
		select * from QXJ_MODLE_DEPT 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from QXJ_MODLE_DEPT 
	</select>
	<select id="findByDept" resultType="com.css.app.qxjgl.business.entity.QxjModleDept">
		select * from QXJ_MODLE_DEPT where DEPT_ID like '%'||#{value}||'%' limit 1
	</select>
	
	<insert id="save" parameterType="com.css.app.qxjgl.business.entity.QxjModleDept">
		insert into QXJ_MODLE_DEPT
		(
			"ID", 
			"MODLE_NAME", 
			"DEPT_ID", 
			"DEPT_NAME", 
			"CREATED_TIME", 
			"MODLE_VALUE",
			"CREATED_USER_ID",
			"UPDATE_TIME"
		)
		values
		(
			#{id}, 
			#{modleName}, 
			#{deptId}, 
			#{deptName}, 
			#{createdTime}, 
			#{modleValue},
			#{createdUserId},
			#{updateTime}
		)
	</insert>
	 
	<update id="update" parameterType="com.css.app.qxjgl.business.entity.QxjModleDept">
		update QXJ_MODLE_DEPT 
		<set>
			<if test="modleName != null">"MODLE_NAME" = #{modleName}, </if>
			<if test="deptId != null">"DEPT_ID" = #{deptId}, </if>
			<if test="deptName != null">"DEPT_NAME" = #{deptName}, </if>
			<if test="createdTime != null">"CREATED_TIME" = #{createdTime}, </if>
			<if test="createdUserId != null">"CREATED_USER_ID" = #{createdUserId}, </if>
			<if test="updateTime != null">"UPDATE_TIME" = #{updateTime}, </if>
			<if test="modleValue != null">"MODLE_VALUE" = #{modleValue}</if>
		</set>
		where ID = #{id}
	</update>
	
	<delete id="delete">
		delete from QXJ_MODLE_DEPT where ID = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from QXJ_MODLE_DEPT where ID in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>