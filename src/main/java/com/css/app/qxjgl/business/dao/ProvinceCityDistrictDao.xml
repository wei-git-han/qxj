<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.app.qxjgl.business.dao.ProvinceCityDistrictDao">

	<select id="queryObject" resultType="com.css.app.qxjgl.business.entity.ProvinceCityDistrict">
		select * from PROVINCE_CITY_DISTRICT where ID = #{value}
	</select>

	<select id="queryList" resultType="com.css.app.qxjgl.business.entity.ProvinceCityDistrict">
		select * from PROVINCE_CITY_DISTRICT 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from PROVINCE_CITY_DISTRICT 
	</select>
	 
	<insert id="save" parameterType="com.css.app.qxjgl.business.entity.ProvinceCityDistrict">
		insert into PROVINCE_CITY_DISTRICT
		(
			"ID", 
			"NAME", 
			"PID"
		)
		values
		(
			#{id}, 
			#{name}, 
			#{pid}
		)
	</insert>
	 
	<update id="update" parameterType="com.css.app.qxjgl.business.entity.ProvinceCityDistrict">
		update PROVINCE_CITY_DISTRICT 
		<set>
			<if test="name != null">"NAME" = #{name}, </if>
			<if test="pid != null">"PID" = #{pid}</if>
		</set>
		where ID = #{id}
	</update>
	
	<delete id="delete">
		delete from PROVINCE_CITY_DISTRICT where ID = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from PROVINCE_CITY_DISTRICT where ID in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>